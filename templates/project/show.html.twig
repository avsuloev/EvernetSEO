{% extends 'base.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
    <h1>Project</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>TvId</th>
                <td>{{ project.tvId }}</td>
            </tr>
            <tr>
                <th>YmId</th>
                <td>{{ project.ymId }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ project.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ project.url }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ project.createdAt ? project.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ project.updatedAt ? project.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CmsTitle</th>
                <td>{{ project.cmsTitle }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('project_index') }}">back to list</a>

    <a href="{{ path('project_edit', {'id': project.id}) }}">edit</a>

    {{ include('project/_delete_form.html.twig') }}
{% endblock %}
